class Solution {
public:
    int trailingZeroes(int n) {
        long long num = 1;
        while(n>0){
            num *= n;
            n--;
        }
        char res;
        string s = "";
        while(n>0){
            res = char(n%10);
            n /= 10;
            s = res + s;
        }
        printf("%s",s);
        
        int count = 0;
        for(int i = s.length()-1;i>0;i--){
            if(s[i]==0) count++; 
        }
        return count;
    }
};


class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        vector<int> data;
        int s = nums.size();
        int l;
        if(s<k){
            l = k%nums.size();
            for(int i = l+1;i<nums.size();i++){
            data.push_back(nums[i]);
            }
            for(int i = 0;i<l+1;i++){
                data.push_back(nums[i]);
            }
        }else{
            l = nums.size()-k;
            for(int i = l;i<nums.size();i++){
            data.push_back(nums[i]);
            }
            for(int i = 0;i<l;i++){
                data.push_back(nums[i]);
            }
        }
        //printf("%d",l);
        
        nums = data;
    }
};

