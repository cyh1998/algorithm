class Solution {
public:
    int trailingZeroes(int n) {
        long long num = 1;
        while(n>0){
            num *= n;
            n--;
        }
        char res;
        string s = "";
        while(n>0){
            res = char(n%10);
            n /= 10;
            s = res + s;
        }
        printf("%s",s);
        
        int count = 0;
        for(int i = s.length()-1;i>0;i--){
            if(s[i]==0) count++; 
        }
        return count;
    }
};
